{
  "name": "swagger-typed-express-docs",
  "version": "0.0.1-beta.2",
  "description": "Simple express runtime parser and documentation swagger generator with 100% support of Typescript static types",
  "keywords": [
    "express",
    "typescript",
    "swagger",
    "documentation",
    "inferring",
    "http",
    "runtime-validation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Svehla/swagger-typed-express-docs"
  },
  "license": "ISC",
  "author": "Jakub Švehla",
  "contributors": [
    "Jakub Švehla"
  ],
  "main": "dist/index.js",
  "browser": {
    "fs": false
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "npm run clean && npm run build",
    "build": "tsc",
    "clean": "tsc --build --clean",
    "lint": "eslint \"{src,tests}/**/*.{js,ts}\"",
    "lint:fix": "eslint \"{src,tests}/**/*{js,ts}\" --fix",
    "dev": "ts-node-dev --respawn example/server.ts",
    "ts:check-types": "tsc",
    "ts:check-types:watch": "tsc --watch",
    "check-code": "npm run test && npm run lint && npm run ts:check-types",
    "publish-lib": "npm run prepare && npm publish"
  },
  "dependencies": {
    "@types/express": "^4.17.12",
    "express-query-parser": "^1.2.0",
    "nodemon": "^2.0.12",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.41",
    "@types/swagger-ui-express": "^4.1.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "swagger-ui-express": "^4.1.6",
    "ts-jest": "^26.5.4",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "express": "^4.17.1"
  }
}
